{
	"@id": "dtmi:tartabit:Device;1",
	"@type": "Interface",
	"displayName": {
		"en": "Device"
	},
	"@context": [
		"dtmi:iotcentral:context;2",
		"dtmi:dtdl:context;2"
	],
