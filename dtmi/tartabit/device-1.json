				"mapValue": {
					"name": "sensor",
					"schema": {
						"@type": "Object",
						"fields": [
							{ "name":"desc", "displayName": {"en": "Description"}, "schema":"string"},
							{ "name":"type", "displayName": {"en": "Type"}, "schema":"string"},
							{ "name":"units", "displayName": {"en": "Units"}, "schema":"string"}
